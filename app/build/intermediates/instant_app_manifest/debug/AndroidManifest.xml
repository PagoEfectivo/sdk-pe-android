<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pagoefectivo_sdk_android"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.pagoefectivo_sdk_android.PagoEfectivoSdkApplication"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.pagoefectivo_sdk_android.ui.activities.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.pagoefectivo_sdk_android.ui.activities.GenerateCipActivity" />
        <activity
            android:name="com.example.pagoefectivo_sdk_android.ui.activities.PaymentMethodActivity"
            android:label="@string/method_payment_name" />
        <activity android:name="com.example.pagoefectivo_sdk_android.ui.activities.WhereToPayActivity" />
        <activity android:name="com.example.pagoefectivo_sdk_android.ui.activities.WebActivity" />
        <activity
            android:name="com.example.pagoefectivo_sdk_android.ui.activities.PaymentDetailActivity"
            android:label="@string/summary_payment_name" />
    </application>

</manifest>